  },
  "maintainers": [
    "Alex Scheitlin <alex.scheitlin@uzh.ch>",
    "Roland Schlaefli <rolandschlaefli@gmail.com>",
    "Nik Zaugg <nik.zaugg@uzh.ch>"
  ],
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "1.5.0",
    "@ionic/react": "5.0.0",
    "@ionic/react-router": "5.0.0",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.4.0",
    "@testing-library/user-event": "8.0.3",
    "@types/jest": "24.0.25",
    "@types/node": "12.12.24",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "@types/react-router": "5.1.4",
    "@types/react-router-dom": "5.1.3",
    "@typescript-eslint/eslint-plugin": "2.21.0",
    "@typescript-eslint/parser": "2.21.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-react": "7.18.3",
    "ionicons": "5.0.0",
    "prettier": "1.19.1",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.0",
    "serve": "11.3.0",
    "typescript": "3.7.4"
  },
  "scripts": {
    "dev": "react-scripts start",
    "start": "serve build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:ci": "eslint \"{src,test}/**/*.{ts,tsx}\"",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx}\" --fix",
    "format:ci": "prettier --list-different \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit -p tsconfig.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@capacitor/cli": "1.5.0"
  }
}
