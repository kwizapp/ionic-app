{
  "name": "ionic-app",
  "version": "1.0.0",
  "description": "Ionic App for Kwiz (Mobile / Web)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kwizapp/ionic-app.git"
  },
  "maintainers": [
    "Alex Scheitlin <alex.scheitlin@uzh.ch>",
    "Roland Schlaefli <rolandschlaefli@gmail.com>",
    "Nik Zaugg <nik.zaugg@uzh.ch>"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development npm-run-all --parallel dev:style dev:app",
    "dev:app": "react-scripts start",
    "dev:style": "postcss src/theme/index.css -o src/theme/tailwind.css --watch",
    "start": "serve build",
    "build": "cross-env NODE_ENV=production npm-run-all build:style build:app",
    "build:app": "react-scripts build",
    "build:style": "postcss src/theme/index.css -o src/theme/tailwind.css",
    "test": "react-scripts test",
    "lint:ci": "eslint \"{src,test}/**/*.{ts,tsx}\"",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx}\" --fix",
    "format:ci": "prettier --list-different \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit -p tsconfig.json",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.0.0-beta.49",
    "@capacitor/cli": "2.1.0",
    "@capacitor/core": "2.1.0",
    "@fullhuman/postcss-purgecss": "2.1.2",
    "@ionic/react": "5.1.0",
    "@ionic/react-router": "5.1.0",
    "@kwizapp/eslint-config-ts-react": "1.0.0",
    "@kwizapp/prettier-config": "1.0.0",
    "@testing-library/jest-dom": "5.11.10",
    "@testing-library/react": "10.0.4",
    "@testing-library/user-event": "10.3.1",
    "@types/classnames": "2.2.10",
    "@types/jest": "25.2.3",
    "@types/node": "14.0.1",
    "@types/node-emoji": "1.8.1",
    "@types/ramda": "0.27.5",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-router": "5.1.7",
    "@types/react-router-dom": "5.1.5",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "classnames": "2.2.6",
    "cross-env": "7.0.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "graphql": "14.6.0",
    "immer": "6.0.5",
    "ionicons": "5.0.1",
    "node-emoji": "1.10.0",
    "npm-run-all": "4.1.5",
    "postcss-cli": "7.1.1",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.2",
    "prettier": "2.0.5",
    "ramda": "0.27.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-number-format": "4.4.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-router-last-location": "2.0.1",
    "react-scripts": "3.4.1",
    "react-spring": "9.0.0-beta.34",
    "react-styleguidist": "11.0.6",
    "react-three-fiber": "4.2.4",
    "tailwindcss": "1.4.6",
    "three": "0.116.1",
    "typescript": "3.8.3",
    "zustand": "2.2.3"
  }
}
