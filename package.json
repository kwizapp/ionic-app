{
  "name": "ionic-app",
  "version": "0.0.1",
  "description": "Ionic App for Kwiz (Mobile / Web)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kwizapp/ionic-app.git"
  },
  "maintainers": [
    "Alex Scheitlin <alex.scheitlin@uzh.ch>",
    "Roland Schlaefli <rolandschlaefli@gmail.com>",
    "Nik Zaugg <nik.zaugg@uzh.ch>"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all --parallel dev:style dev:app",
    "dev:app": "react-scripts start",
    "dev:style": "postcss src/theme/index.css -o src/theme/tailwind.css --watch",
    "start": "serve build",
    "build": "npm-run-all build:style build:app",
    "build:app": "react-scripts build",
    "build:style": "postcss src/theme/index.css -o src/theme/tailwind.css",
    "test": "react-scripts test",
    "lint:ci": "eslint \"{src,test}/**/*.{ts,tsx}\"",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx}\" --fix",
    "format:ci": "prettier --list-different \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit -p tsconfig.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.0.0-beta.41",
    "@capacitor/cli": "1.5.1",
    "@capacitor/core": "1.5.1",
    "@fullhuman/postcss-purgecss": "2.1.0",
    "@ionic/react": "5.0.7",
    "@ionic/react-router": "5.0.7",
    "@kwizapp/eslint-config-ts-react": "0.0.10",
    "@kwizapp/prettier-config": "0.0.10",
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "9.5.0",
    "@testing-library/user-event": "10.0.0",
    "@types/jest": "25.1.4",
    "@types/node": "12.12.24",
    "@types/node-emoji": "^1.8.1",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/react-router": "5.1.4",
    "@types/react-router-dom": "5.1.3",
    "@typescript-eslint/eslint-plugin": "2.23.0",
    "@typescript-eslint/parser": "2.28.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "graphql": "14.6.0",
    "immer": "6.0.3",
    "ionicons": "5.0.1",
    "node-emoji": "^1.10.0",
    "npm-run-all": "4.1.5",
    "postcss-cli": "7.1.0",
    "postcss-flexbugs-fixes": "4.2.0",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.2",
    "prettier": "1.19.1",
    "ramda": "0.27.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.0",
    "tailwindcss": "1.2.0",
    "typescript": "3.8.3",
    "zustand": "2.2.3"
  }
}
